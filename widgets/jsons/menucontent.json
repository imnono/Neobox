{
  "设置中心": {
    "type": "Group",
    "children": {
      "软件设置": {
        "type": "Group",
        "children": {
          "开机自启": {
            "type": "Checkable",
            "function": "AppAutoSatrt"
          },
          "窗口效果": {
            "type": "ExclusiveGroup",
            "function": "AppFormEffect",
            "range": [0, -1],
            "children": {
              "普通效果": {
                "type": "GroupItem",
                "function": "AppFormEffect",
                "index": 0
              },
              "渐变效果": {
                "type": "GroupItem",
                "function": "AppFormEffect",
                "index": 1
              },
              "透明效果": {
                "type": "GroupItem",
                "function": "AppFormEffect",
                "index": 2
              },
              "磨砂效果": {
                "type": "GroupItem",
                "function": "AppFormEffect",
                "index": 3
              },
              "亚克力效果": {
                "type": "GroupItem",
                "function": "AppFormEffect",
                "index": 4
              }
            }
          },
          "背景颜色": {
            "type": "Normal",
            "function": "AppFormBackground"
          }
        }
      },
      "壁纸设置": {
        "type": "Group",
        "children": {
          "类型选择": {
            "type": "ExclusiveGroup",
            "range": [0, -1],
            "function": "WallpaperType",
            "children": {
              "壁纸天堂": {
                "type": "GroupItem", 
                "function": "WallpaperType", 
                "index": 0
              },
              "必应壁纸": {
                "type": "GroupItem",
                "function": "WallpaperType",
                "index": 1 
              },
              "直链壁纸": {
                "type": "GroupItem",
                "function": "WallpaperType",
                "index": 2
              },
              "本地壁纸": {
                "type": "GroupItem",
                "function": "WallpaperType",
                "index": 3 },
              "脚本输出": {
                "type": "GroupItem",
                "function": "WallpaperType",
                "index": 4
              },
              "收藏喜欢": {
                "type": "GroupItem",
                "function": "WallpaperType",
                "index": 5
              }
            }
          },
          "自动更换": {
            "type": "Checkable",
            "function": "WallpaperAutoChange"
          },
          "首次更换": {
            "type": "Checkable",
            "function": "WallpaperInitChange"
          },
          "时间间隔": {
            "type": "Normal",
            "function": "WallpaperTimeInterval"
          },
          "存储位置": {
            "type": "Normal",
            "function": "WallpaperDir"
          },
          "清理垃圾": {
            "type": "Normal",
            "function": "WallpaperClean"
          },
          "更多设置": {
            "type": "VarGroup",
            "name": "Wallpaper"
          }
        }
      },
      "插件设置": {
        "type": "Group",
        "children":{
          "翻译设置": {
            "type": "Group",
            "children": {
              "数据位置": {
                "type": "Normal",
                "function": "ToolOcrDataPath"
              },
              "翻译热键": {
                "type": "Group",
                "children": {
                  "热键名称": {
                    "type": "Normal",
                    "function": "ToolTransShortcut"
                  },
                  "注册热键": {
                    "type": "Checkable",
                    "function": "ToolTransRegistKey"
                  }
                }
              },
              "截屏热键": {
                "type": "Group",
                "children": {
                  "热键名称": {
                    "type": "Normal",
                    "function": "ToolOcrShortcut"
                  },
                  "注册热键": {
                    "type": "Checkable",
                    "function": "ToolOcrRegistKey"
                  }
                }
              }
            }
          }
        }
      },
      "系统设置": {
        "type": "Group",
        "children": {
          "右键复制": {
            "type": "Checkable",
            "function": "OtherSetDesktopRightMenu"
          }
        }
      },
      "其他设置": {
        "type": "Group",
        "children": {
          "关于软件": {
            "type": "Normal",
            "function": "AppWbsite"
          }
        }
      }
    }
  },
  "翻译功能": {
    "type": "Group",
    "children": {
      "翻译窗口": {
        "type": "Normal",
        "function": "ToolTransShowDlg"
      },
      "截图翻译": {
        "type": "Normal",
        "function": "ToolOcrGetScreen"
      }
    }
  },
  "壁纸切换": {
    "type": "Group",
    "children": {
      "上一张图": {
        "type": "Normal",
        "function": "WallpaperPrev"
      },
      "下一张图": {
        "type": "Normal",
        "function": "WallpaperNext"
      },
      "不看此图": {
        "type": "Normal",
        "function": "WallpaperDislike"
      },
      "撤销删除": {
        "type": "Normal",
        "function": "WallpaperUndoDislike"
      },
      "收藏图片": {
        "type": "Normal",
        "function": "WallpaperCollect"
      },
      "取消收藏": {
        "type": "Normal",
        "function": "WallpaperUndoCollect"
      }
    }
  },
  "打开位置": {
    "type":"Group",
    "children": {
      "程序目录": {
        "type": "Normal",
        "function": "AppOpenExeDir"
      },
      "配置目录": {
        "type": "Normal",
        "function": "AppOpenConfigDir"
      },
      "壁纸目录": {
        "type": "Normal",
        "function": "AppOpenWallpaperDir"
      },
      "壁纸位置": {
        "type": "Normal",
        "function": "AppOpenWallpaperLocation"
      }
    }
  },
  "开机自启": {
    "type": "Checkable",
    "function": "AppAutoSatrt"
  },
  "防止息屏": {
    "type": "Checkable",
    "function": "SystemStopSleep" 
  },
  "快速关机": {
    "type": "Normal",
    "function": "SystemShutdown"
  },
  "快捷重启": {
    "type": "Normal",
    "function": "SystemRestart"
  },
  "退出软件": {
    "type":"Normal",
    "function": "AppQuit"
  }
}
